	measure Flag_Ultimos7Dias =
			
			VAR _Hoy = TODAY()
			RETURN
			IF(
			    MAX( Calendario[Date] ) >= _Hoy - 7 &&
			    MAX( Calendario[Date] ) <= _Hoy,
			    1,
			    0
			)
